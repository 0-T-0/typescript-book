{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./assertion.ts",
        "./freshness/freshness.ts",
        "./functions.ts",
        "./lib/exclude/nolibd.ts",
        "./lib/usage/libd.ts",
        "./migrating/migrating.ts",
        "./readonly.ts",
        "./stringLiteralType.ts",
        "./typeGuard.ts",
        "./types.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
